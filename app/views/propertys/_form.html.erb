<%= form_for(@property ,url: route_post ) do |f| %>
	    <%= f.label :物件名%><br>
	    <%= f.text_field :name %><br>
	    <%= f.label :賃料 %><br>
	    <%= f.text_field :price %>円<br>
	    <%= f.label :住所 %><br>
	    <%= f.text_field :address %><br>
	    <%= f.label :築年数 %><br>
	    <%= f.text_field :age %>年<br>
	    <%= f.label :備考 %><br>
	    <%= f.text_area :remarks %><br>
			<hr>
			<div class="nearests">
      <%= f.fields_for :nearests do |a| %>
				<%= render "nearest_fields", f: a %>
      <% end %>
			<div class="links">
			  <%= link_to_add_association "最寄り駅を追加", f,:nearests%>
			</div>

	    <%= f.submit %>
<% end %>
