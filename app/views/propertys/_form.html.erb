<%= form_for(@property) do |f| %>
	    <%= f.label :物件名%><br>
	    <%= f.text_field :name %><br>
	    <%= f.label :賃料 %><br>
	    <%= f.text_field :price %>円<br>
	    <%= f.label :住所 %><br>
	    <%= f.text_field :address %><br>
	    <%= f.label :築年数 %><br>
	    <%= f.text_field :age %>年<br>
	    <%= f.label :備考 %><br>
	    <%= f.text_area :remarks %><br>
			<hr>
      <%= f.fields_for :nearests do |a| %>
			  <%= a.label :路線名 %><br>
			  <%= a.text_field :route_name %><br>
				<%= a.label :駅名 %><br>
			  <%= a.text_field :station %><br>
				<%= a.label :徒歩分数 %><br>
				<%= a.text_field :time %>分<br>
      <% end %>
	    <%= f.submit %>
<% end %>
