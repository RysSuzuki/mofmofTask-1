<h1>閲覧</h1>
  <dev>
    <p>物件名: <%= @property.name %></p>
    <p>賃料: <%= @property.price %>円</p>
    <p>住所: <%= @property.address %></p>
    <p>築年数: <%= @property.age %>年</p>
    <p>備考: <%= @property.remarks %></p>
  </dev>
  <hr>
  <% @property.nearests.each_with_index do | nearest,i | %>
   <h3>最寄り駅<%= i+=1 %></h3>
    <p>路線名  <%= nearest.route_name %></p>
    <p>駅名  <%= nearest.station %></p>
    <p>徒歩分数  <%= nearest.time %>分</p>
  <% end %>
  <%= link_to '編集', edit_property_path(@property) %>|
  <%= link_to '戻る', propertys_path %>
